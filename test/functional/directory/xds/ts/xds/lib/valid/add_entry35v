-- TESTCASE:
--   add_entry35v

-- CATEGORY:
--   valid behaviour test

-- DESCRIPTION:
--   Test the XDS function ds_add_entry().
--   Add an OrganizationalPerson object with Strong Auth. User attribute:
--     name (C=ie/O=sni/OU=sci/CN=sap),
--     entry (objectClass         = (SA-User,
--                                   OrganizationalPerson,
--                                   Person,
--                                   TOP),
--            surname             = sap,
--            userCertificate     = (SIGNED 
--                                   (v1988,
--                                    11111, 
--                                    (2.5.8.1), 
--                                    /C=ie/O=sni, 
--                                    (199001011200, 200001011200),
--                                    /C=ie/O=sni/OU=sci/CN=sap, 
--                                    ((2.5.8.1), 
--                                     \x01\x02\x03\x04\x05
--                                    ),
--                                   ),
--                                  ).
  

-- PROCEDURE:
--   Build test-tree.
--   Initialise XDS.
--   Negotiate the BDC, MDU, SAP and GDS packages.
--   Bind to the default DSA.
--   Add entries.
--   Unbind from the default DSA.
--   Shutdown.
--   Remove test-tree.

--------------------------------------------------------------------------------

Include  ../tree/build_tree8

Function INITIALIZE
	workspace1

Function VERSION
	feature_list5		
	workspace1
	DS_SUCCESS

Function BIND
	default_session
    workspace1
	DS_SUCCESS
	bound_session1

Function ADD_ENTRY
	bound_session1
	default_context
	name92
	attribute_list62
	DS_SUCCESS

Function REMOVE_ENTRY
	bound_session1
	default_context
	name92
	DS_SUCCESS

Function UNBIND
	bound_session1
	DS_SUCCESS

Function SHUTDOWN
	workspace1
	DS_SUCCESS 

Include  ../tree/delete_tree8

