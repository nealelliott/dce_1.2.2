set test_name "append1"
set result "12abclong string"
catch {unset x}
list [append x 1 2 abc "long string"] $x
if {[string compare $x $result] == 0} then { 
	puts stdout "++++ $test_name PASSED" 
} else {
	puts stdout "++++ $test_name FAILED"
}
