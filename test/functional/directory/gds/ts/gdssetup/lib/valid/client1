#Configure Dir-id 3 as Client-System
#Add 2 default DSA's(dsa-m7 and dsa-m8) in cache

Dir-id> "3"
Config-type(C/S)> "c"
Client address> "TS=Client,NA='TCP/IP!internet=127.0.0.1+port=30030'"

Default-DSA name<1>> "/C=de/O=dbp/OU=dap11/CN=dsa/CN=dsa-m8"
PSAP address <1>> "TS=Client,NA='TCP/IP!internet=127.0.0.1+port=30081'"
Default-DSA name<2>> "/C=de/O=dbp/OU=dap11/CN=dsa/CN=dsa-m7"
PSAP address <2>> "TS=Client,NA='TCP/IP!internet=127.0.0.1+port=30071'"
