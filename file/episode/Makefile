SUBDIRS	       = async logbuf \
		 anode dir \
		 vnops salvage libefs utils sautils \
		 ravage scavenge rcx tools

SAMIDL_SUBDIRS = salvage

EXPINC_SUBDIRS = tools async anode logbuf dir eacl vnops

EXPLIB_SUBDIRS = tools tools.klib async async.klib logbuf logbuf.klib \
		 anode anode.klib dir dir.klib eacl eacl.klib \
		 vnops vnops.klib libefs

.if exists(${MAKETOP}/file/${TARGET_MACHINE}/machdep.mk)
.include "${MAKETOP}/file/${TARGET_MACHINE}/machdep.mk"
.endif
.if exists(${MAKETOP}/file/${TARGET_OS}/machdep.mk)
.include "${MAKETOP}/file/${TARGET_OS}/machdep.mk"
.endif

.if exists (${TARGET_MACHINE}/Makefile)
.include "${TARGET_MACHINE}/Makefile"
.endif
.if exists (${TARGET_OS}/Makefile)
.include "${TARGET_OS}/Makefile"
.endif

.include <${RULES_MK}>
