-- TESTCASE:
--   mod_entry4v

-- CATEGORY:
--   valid behaviour test

-- DESCRIPTION:
--   Test the XDS function ds_modify_entry().
--   Modify an OrganizationalPerson object:
--     name (C=ie/O=sni/OU=sci/CN=vinnie),
--     changes (remove values, telephoneNumber=+353 1 767551 x999).

-- PROCEDURE:
--   Build test-tree.
--   Initialise XDS.
--   Negotiate the BDC and GDS packages.
--   Bind to the default DSA.
--   Modify entry.
--   Modify entry.
--   Read entry.
--   Modify entry.
--   Read entry.
--   Unbind from the default DSA.
--   Shutdown.
--   Remove test-tree.



--------------------------------------------------------------------------------

Include  ../tree/build_tree8

Function INITIALIZE
	workspace1

Function VERSION
	feature_list1		
        workspace1
	DS_SUCCESS

Function BIND
	default_session
        workspace1
	DS_SUCCESS
	bound_session1

Function MODIFY_ENTRY
	bound_session1
	default_context
	name8
	entry_modification_list6
	DS_SUCCESS

Function MODIFY_ENTRY
	bound_session1
	default_context
	name8
	entry_modification_list8
	DS_SUCCESS

Function READ
	bound_session1
	default_context
	name8
	entry_info_selection1
	DS_SUCCESS
        read_result_return43

Function MODIFY_ENTRY
	bound_session1
	default_context
	name8
	entry_modification_list9
	DS_SUCCESS

Function READ
	bound_session1
	default_context
	name8
	entry_info_selection1
	DS_SUCCESS
        read_result_return42

Function UNBIND
	bound_session1
	DS_SUCCESS

Function SHUTDOWN
	workspace1
        DS_SUCCESS 

Include  ../tree/delete_tree8
