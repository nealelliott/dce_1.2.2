	check_item_done "$?"
	check_item_begin "GGCMD-09-12"
	remove_from_group_by_rgy_edit $gG /.../$cR/$pRs
	check_item_done "$?"
	check_item_begin "GGCMD-09-13"
	add_to_group_by_rgy_edit $gG /.../$cR/$pRl
