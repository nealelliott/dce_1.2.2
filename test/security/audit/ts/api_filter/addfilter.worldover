if (( $AUDITCP ))
then
 auditcp audfilter add <<EOF
7
success

log

dce_audit_filter_query
EOF

else
  dcecp << EOF
audfilter create world_overridable  -attribute {dce_audit_filter_query success log}
audfilter show world_overridable
EOF
fi

echo "auditcp done" > /tmp/done
