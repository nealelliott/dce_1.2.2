set test_name "eval1"
set result "a b c d e f g"
catch {unset x}
set x [eval concat a b c d e f g]
if {[string compare $x $result] == 0} then { 
	puts stdout "++++ $test_name PASSED" 
} else {
	puts stdout "++++ $test_name FAILED"
}
#
#
set test_name "eval2"
set result "22"
catch {unset x}
set x [eval {format 22}]
if {[string compare $x $result] == 0} then {
	puts stdout "++++ $test_name PASSED"
} else {
	puts stdout "++++ $test_name FAILED"
}
#
#
set test_name "eval3"
set result "xyzzy"
catch {unset x}
set x [set a {$b}
	set b xyzzy
	eval format $a]
if {[string compare $x $result] == 0} then {
	puts stdout "++++ $test_name PASSED"
} else {
	puts stdout "++++ $test_name FAILED"
}
