-- TESTCASE:
--   read29v

-- CATEGORY:
--   valid behaviour test

-- DESCRIPTION:
--   Test the XDS function ds_read().
--   Read an Organization object:
--     name (C=ie/O=sni),
--     entry ( objectClass=Organization,
--             facsimileTelephoneNumber=( +49 89 636 51 ),
--             postalAddress=( SNI, Otto-Hahn-Ring 6, D-8000 Munich 83 ),
--             searchGuide=( ITEM: EQUALITY: objectClass ),
--             teletexTerminalIdentifier=( 898250=siemcp ),
--             telexNumber=( SNI, d, 987654321 ) ).


-- PROCEDURE:
--   Build test-tree.
--   Initialise XDS.
--   Negotiate the BDC and GDS packages.
--   Bind to the default DSA.
--   Read entry.
--   Unbind from the default DSA.
--   Shutdown.
--   Remove test-tree.



--------------------------------------------------------------------------------

Include  ../tree/build_tree1

Function INITIALIZE
	workspace1

Function VERSION
	feature_list1		
        workspace1
	DS_SUCCESS

Function BIND
	default_session
        workspace1
	DS_SUCCESS
	bound_session1

Function ADD_ENTRY
	bound_session1
	default_context
	name2
	attribute_list29
	DS_SUCCESS

Function READ
	bound_session1
	default_context
	name2
        entry_info_selection1
	DS_SUCCESS
        read_result_return19

Function REMOVE_ENTRY
	bound_session1
	default_context
	name2
	DS_SUCCESS

Function UNBIND
	bound_session1
	DS_SUCCESS

Function SHUTDOWN
	workspace1
        DS_SUCCESS 

Include  ../tree/delete_tree1
