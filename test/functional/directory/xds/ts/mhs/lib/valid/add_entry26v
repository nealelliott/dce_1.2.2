-- PROCEDURE:
--   Build test-tree.
--   Initialise XDS.
--   Negotiate the BDC, MDU and GDS packages.
--   Bind to the default DSA.
--   Add entries.
--   Unbind from the default DSA.
--   Shutdown.
--   Remove test-tree.


--------------------------------------------------------------------------------

Include  ../tree/build_tree8

Function INITIALIZE
	workspace1

Function VERSION
	feature_list4		
        workspace1
	DS_SUCCESS

Function BIND
	default_session
        workspace1
	DS_SUCCESS
	bound_session1

Function ADD_ENTRY
	bound_session1
	default_context
	name73
	attribute_list49
	DS_SUCCESS

Function REMOVE_ENTRY
	bound_session1
	default_context
	name73
	DS_SUCCESS

Function UNBIND
	bound_session1
	DS_SUCCESS

Function SHUTDOWN
	workspace1
        DS_SUCCESS 

Include  ../tree/delete_tree8
