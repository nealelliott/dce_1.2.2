# POLICY_GET.TSH
#
#  Perform tests for the NCS V2.0 function sec_rgy_plcy_get_info()

echo
echo Begin policy get info tests.
echo 

#
# Import the environment and literal definition files
#
include tshrc
include policy.tshrc

set TEST=policy

#
# Begin sec_rgy_plcy_get_info tests.
#

test $TEST \
"1 All valid data" \
$GET_POLICY $sec_rgy_status_ok

test $TEST \
"2 organization does not exist" \
$GET_POLICY_NOT_FOUND $sec_rgy_object_not_found

test $TEST \
"3 get policy for rgy" \
$GET_POLICY_RGY $sec_rgy_status_ok

test $TEST \
"4 get policy with wildcard" \
$GET_POLICY_WILDCARD $sec_rgy_status_ok

echo

subtotal clear
summary

echo 
echo End policy get info tests.
echo

# End of policy_get.tsh
