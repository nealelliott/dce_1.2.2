set test_name "case1"
set result "1"
catch {unset x}
set x [case a in a {format 1} b {format 2} c {format 3} default {format 4}]
if {[string compare $x $result] == 0} then { 
	puts stdout "++++ $test_name PASSED" 
} else {
	puts stdout "++++ $test_name FAILED"
}
