-- TESTCASE:
--   mod_rdn5v

-- CATEGORY:
--   valid behaviour test

-- DESCRIPTION:
--   Test the XDS function ds_modify_rdn().
--   Modify RDN of an Alias object:
--     name (C=ie/O=sni/OU=sci/CN=vincent),
--     new-RDN (CN=vin),
--     delete-old-RDN=off.

-- PROCEDURE:
--   Build test-tree.
--   Initialise XDS.
--   Negotiate the BDC and GDS packages.
--   Bind to the default DSA.
--   Modify entry.
--   Read entry.
--   Modify entry.
--   Read entry.
--   Unbind from the default DSA.
--   Shutdown.
--   Remove test-tree.



--------------------------------------------------------------------------------

Include  ../tree/build_tree2

Function INITIALIZE
	workspace1

Function VERSION
	feature_list1		
        workspace1
	DS_SUCCESS

Function BIND
	default_session
        workspace1
	DS_SUCCESS
	bound_session1

Function MODIFY_RDN
	bound_session1
	default_context
	name19
	rdn29
	FALSE
	DS_SUCCESS

Function READ
	bound_session1
	default_context
	name39
	entry_info_selection1
	DS_SUCCESS
        read_result_return47

Function MODIFY_RDN
	bound_session1
	default_context
	name39
	rdn20
	TRUE
	DS_SUCCESS

Function READ
	bound_session1
	default_context
	name19
	entry_info_selection1
	DS_SUCCESS
        read_result_return9

Function UNBIND
	bound_session1
	DS_SUCCESS

Function SHUTDOWN
	workspace1
        DS_SUCCESS 

Include  ../tree/delete_tree2
