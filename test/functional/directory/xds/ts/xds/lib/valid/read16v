-- TESTCASE:
--   read16v

-- CATEGORY:
--   valid behaviour test

-- DESCRIPTION:
--   Test the XDS function ds_read().
--   Read an organizationalPerson object:
--     context (priority=high),
--     name (C=ie/O=sni/OU=sci/CN=vinnie),
--     selection (all attributes, types/values).

-- PROCEDURE:
--   Build test-tree.
--   Initialise XDS.
--   Negotiate the BDC and GDS packages.
--   Create a private context object (DSX_C_GDS_CONTEXT) using om_create().
--   Modify the private context from a public context using om_put().
--   Bind to the default DSA.
--   Read entry.
--   Unbind from the default DSA.
--   Shutdown.
--   Remove test-tree.



--------------------------------------------------------------------------------

Include  ../tree/build_tree8

Function INITIALIZE
	workspace1

Function VERSION
	feature_list1		
        workspace1
	DS_SUCCESS

Function CREATE
	DSX_C_GDS_CONTEXT
	TRUE
	workspace1
	OM_SUCCESS
	private_object1

Function PUT
	private_object1
	OM_REPLACE_ALL
	context_mod9
	type_list1
	0
	0
	OM_SUCCESS

Function BIND
	default_session
        workspace1
	DS_SUCCESS
	bound_session1

Function READ
	bound_session1
	private_object1
	name8
	entry_info_selection1
	DS_SUCCESS
    read_result_return8

Function UNBIND
	bound_session1
	DS_SUCCESS

Function SHUTDOWN
	workspace1
        DS_SUCCESS 

Include  ../tree/delete_tree8


