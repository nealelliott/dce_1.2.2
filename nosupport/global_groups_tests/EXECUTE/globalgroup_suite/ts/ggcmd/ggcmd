	check_item_done "$?"
	check_item_begin "GGCMD-08-04"
	view_group_members_by_dcecp $gG -m /.../$cR/$pR
	check_item_done "$?"
	check_item_begin "GGCMD-08-05"
	remove_from_group_by_dcecp $gG -m /.../$cR/$pR
