set test_name "file1"
set result ".def"
catch {unset x}
set x [file ext abc.def]
if {[string compare $x $result] == 0} then { 
	puts stdout "++++ $test_name PASSED" 
} else {
	puts stdout "++++ $test_name FAILED"
}
