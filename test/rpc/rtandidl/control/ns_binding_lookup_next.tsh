echo
echo Begin ns_binding_lookup_next tests.
echo 
#
#
#
# Import the environment and literal deefinition files
#
include tshrc
include ns.tshrc
#
set ns_binding_lookup=ns_binding_lookup
#
#
# Set the default environmental variables for the tests that use them
#
#
#
test $ns_binding_lookup \
"1 next lookup with good inteface, good object uuid and default binding_max_count" \
$BINDING_LOOKUP_NEXT $rpc_s_ok \
$rpc_c_ns_syntax_dce  NSTEST_DIR/profile_test_export1_1 \
$MAKE_GOOD_IF_SPEC $MAKE_GOOD_UUID_VECTOR $rpc_c_binding_max_count_default $PROTOCOL
#
test $ns_binding_lookup \
"2 next lookup with good interface, NULL object uuid and default binding_max_count" \
$BINDING_LOOKUP_NEXT $rpc_s_ok \
$rpc_c_ns_syntax_dce  NSTEST_DIR/profile_test_export1_1 \
$MAKE_GOOD_IF_SPEC $MAKE_NULL_UUID_VECTOR $rpc_c_binding_max_count_default $PROTOCOL
#
test $ns_binding_lookup \
"3 next lookup with good interface, NULL object uuid and default binding_max_count" \
$BINDING_LOOKUP_NEXT $rpc_s_no_more_bindings \
$rpc_c_ns_syntax_dce  NSTEST_DIR/profile_test_export1_1 \
$MAKE_GOOD_IF_SPEC $MAKE_GOOD_UUID_VECTOR $rpc_c_binding_max_count_default $PROTOCOL
#

#
#
#
echo
subtotal clear
summary
#
#
echo 
echo End ns_binding_lookup_next tests.
echo
#
# End of ns_binding_lookup.tsh
