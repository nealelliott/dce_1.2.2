######################################
# Data for administrative principles #
###################################### 
#Number of adminstrators
ADMNUM=5

#Prefix for adminstrative principals
ADMNAM="st_admin"

#List of adminstrative principals
ADM_PRINCS="st_admin1 st_admin2 st_admin3 st_admin4 st_admin5"

#Adminstrtive password
ADM_PWS="sta"

#Admin grp
ADMG="systest/st_admin"

#################################
#      Data for principals      #
#################################
#Prefix for principals
PNAM="st_p"

#Number of principals
PNUM=10

#Passwrd for principals
PWS="st"

##############################
#      Miscellaneous         #
##############################
#Set to true if only one STATE wanted (i.e. st1)
ONE_STATE="false"

#Set to 10 in order to run all STATE files.  If ONE_STATE="true"
#set this to the st# file you want.
STNUM=10

#Provides extra info
DEBUG=0

#set variable for ops_parsr.ksh
PARSER="$TET_ROOT/cds/ts/dcecdsacl/ops_parsr.ksh"

#Setup variables
SETUP_PRINC="cell_admin"
SETUP_PW="-dce-"
CELLS=""
GRP="st_tg"
ORG="st_osf"

