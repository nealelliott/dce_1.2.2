sec.functions_IDIR	= /tet/security/ts/

dlgcfg001_IDIR		= /tet/security/ts/dlgcfg001/
dlg_kta_IDIR		= /tet/security/ts/dlgcfg001/
dlg_ktd_IDIR		= /tet/security/ts/dlgcfg001/
dlg_start_intermediary_IDIR	= /tet/security/ts/dlgcfg001/

dceseacl_IDIR		= /tet/security/ts/dceseacl/
dceseacl.data_IDIR	= /tet/security/ts/dceseacl/

dceseact_IDIR		= /tet/security/ts/dceseact/
dceseact.data_IDIR	= /tet/security/ts/dceseact/

dcesepol_IDIR		= /tet/security/ts/dcesepol/
dcesepol.data_IDIR	= /tet/security/ts/dcesepol/

dcesestr_IDIR		= /tet/security/ts/dcesestr/
dcesestr.data_IDIR	= /tet/security/ts/dcesestr/

resestr_IDIR		= /tet/security/ts/resestr/

tet_code_IDIR		= /tet/security/
tet_scen_IDIR		= /tet/security/
tetbuild.cfg_IDIR	= /tet/security/
tetclean.cfg_IDIR	= /tet/security/
tetexec.cfg_IDIR	= /tet/security/

era.functions_IDIR	= /tet/security/ts/
era.data_IDIR		= /tet/security/ts/

erarel001_IDIR		= /tet/security/ts/erarel001/
erarel001.data_IDIR	= /tet/security/ts/erarel001/

eraobj001_IDIR		= /tet/security/ts/eraobj001/
eraobj001.data_IDIR	= /tet/security/ts/eraobj001/

ILIST   		= ${SCRIPTS}

SCRIPTS			= dcesestr dcesestr.data resestr \
		          dceseacl dceseacl.data dcesepol dcesepol.data \
			  dceseact dceseact.data sec.functions dlgcfg001 \
			  dlg_kta dlg_ktd dlg_start_intermediary \
			  erarel001 eraobj001 era.functions era.data \
			  tet_scen tet_code \
			  tetbuild.cfg tetclean.cfg tetexec.cfg

.include <${RULES_MK}>
